@model List<ClothingVM>

@foreach (ClothingVM item in Model)
{
<div class="col-xs-4">
    <div class="single-product">
        <div class="col-xs-12">
            <div class="product-img ">
                <a asp-controller="Home" asp-action="ClothingDetails" asp-route-id="@item.ClothingID" >
                    <img src=@item.ClothingArtUrl alt="Product Title" />
                </a>
            </div>

            <div class="product-dsc">
                <h3><a href="#">@item.Name</a></h3>
                <div class="star-price">
                    <span class="price-left">@item.Price</span>
                </div>
            </div>
            <div class="actions-btn">
                @*<a id="addcart" href="#" data-toggle="tooltip" data-placement="top" title="Add To Cart"><i class="fa fa-shopping-cart"></i></a>*@
                <a asp-controller="Home" asp-action="ClothingDetails" asp-route-id="@item.ClothingID"  ><i class="fa fa-eye"></i></a>
            </div>
        </div>
    </div>
    
</div>

   



}
